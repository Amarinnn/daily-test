<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jy.exam.mapper.ReceiptMapper">

    <insert id="insertReceipt" parameterType="ReceiptData">
        insert into receiptdata values(DEFAULT,#{blockHash},#{blockNumber},#{effectiveGasPrice},#{from},#{gas},#{gasPrice},#{gasUsed},#{senderTxHash},#{to},#{transactionHash},#{value})
    </insert>

</mapper>